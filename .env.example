# Database Configuration
DATABASE_URL=postgresql://username:password@hostname:port/database_name
PGHOST=your_postgres_host
PGPORT=5432
PGUSER=your_postgres_user
PGPASSWORD=your_postgres_password
PGDATABASE=your_database_name

# Authentication Configuration (if using OpenID Connect)
SESSION_SECRET=your_session_secret_key_minimum_32_characters
ISSUER_URL=https://your-domain.com/oidc
REPL_ID=your_application_id
REPLIT_DOMAINS=your-domain.com,www.your-domain.com

# Application Configuration
NODE_ENV=production
PORT=5000

# Optional: Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100